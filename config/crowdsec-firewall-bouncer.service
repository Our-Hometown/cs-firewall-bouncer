[Unit]
Description=The firewall bouncer for CrowdSec
After=syslog.target network.target remote-fs.target nss-lookup.target crowdsec.service

[Service]
Type=notify
ExecStart=${BIN} -c ${CFG}crowdsec-firewall-bouncer-oht.yaml
ExecStartPre=${BIN} -c ${CFG}crowdsec-firewall-bouncer-oht.yaml -t
ExecStartPost=/bin/sleep 0.1

[Install]
WantedBy=multi-user.target


